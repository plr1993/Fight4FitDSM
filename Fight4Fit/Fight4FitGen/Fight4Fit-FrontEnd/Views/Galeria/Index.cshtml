@model IEnumerable<Fight4FitGenNHibernate.EN.Fight4Fit.GaleriaEN>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreGaleria)
        </th>
        <th>@Html.DisplayNameFor(model => model.Evento)</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreGaleria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Evento.Id)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
    
}

</table>
