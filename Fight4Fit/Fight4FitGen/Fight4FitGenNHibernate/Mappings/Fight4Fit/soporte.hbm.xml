<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Fight4FitGenNHibernate"
		                   namespace="Fight4FitGenNHibernate.EN.Fight4Fit">
		    
		    
			
				<class name="SoporteEN" table="Soporte"> 
				  	
						<id name="Id" column="idSoporte" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
		  			
						<bag name="Usuario" table="[soporte_usuario]"  >
							<key>
								
									<column name="FK_idSoporte_idSoporte"/> 
							</key>
      						<many-to-many class="Fight4FitGenNHibernate.EN.Fight4Fit.UsuarioEN">
									<column name="FK_Email_idUsuario"/> 
      						</many-to-many>
    					</bag>
			
					
					
		
		 </class>	
		  
		  <query name="SoporteENReadAllHQL">from SoporteEN</query>
		</hibernate-mapping>
	