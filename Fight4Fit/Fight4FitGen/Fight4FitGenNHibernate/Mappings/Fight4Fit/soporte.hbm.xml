<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Fight4FitGenNHibernate"
		                   namespace="Fight4FitGenNHibernate.EN.Fight4Fit">
		    
		    
			
				<class name="SoporteEN" table="Soporte"> 
				  	
						<id name="Id" column="idSoporte" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Titulo" type="string"> 
						<column name="Titulo"  not-null="true"/>
					</property>
					<property name="Texto" type="string"> 
						<column name="Texto"  not-null="true"/>
					</property>
					<property name="Respuesta" type="string"> 
						<column name="Respuesta"  not-null="true"/>
					</property>
			
			
		  			
						<many-to-one name="Usuario" class="Fight4FitGenNHibernate.EN.Fight4Fit.UsuarioEN" >	
								<column name="FK_Email_idUsuario"/> 
						</many-to-one>
											 					
			
			
		  			
						<bag name="Admin" table="[soporte_0_admin]"  >
							<key>
								
									<column name="FK_idSoporte_idSoporte_0"/> 
							</key>
      						<many-to-many class="Fight4FitGenNHibernate.EN.Fight4Fit.AdminEN">
									<column name="FK_Email_idAdmin"/> 
      						</many-to-many>
    					</bag>
			
			
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="SoporteENReadAllHQL">from SoporteEN</query>
		</hibernate-mapping>
	