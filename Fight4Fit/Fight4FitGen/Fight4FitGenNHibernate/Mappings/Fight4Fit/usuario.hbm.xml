<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Fight4FitGenNHibernate"
		                   namespace="Fight4FitGenNHibernate.EN.Fight4Fit">
		    
		    
			
				<class name="UsuarioEN" table="Usuario"> 
						<id name="Email" column="Email"/> 
			
					
					<property name="Password" type="string"> 
						<column name="Password"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<many-to-one name="Comentario" class="Fight4FitGenNHibernate.EN.Fight4Fit.ComentarioEN" >	
								<column name="FK_idComentario_idComentario"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Soporte" table="[soporte_usuario]" inverse="true" >
							<key>
								
									<column name="FK_Email_idUsuario"/> 
							</key>
      						<many-to-many class="Fight4FitGenNHibernate.EN.Fight4Fit.SoporteEN">
									<column name="FK_idSoporte_idSoporte"/> 
      						</many-to-many>
    					</bag>
					
					
					
					
		
		 </class>	
		  
		  <query name="UsuarioENReadAllHQL">from UsuarioEN</query>
		</hibernate-mapping>
	